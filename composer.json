{
    "name": "zhooravell/php-monobank",
    "description": "PHP client for Monobank open API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Roman Zhuravel",
            "email": "zhooravell@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-filter": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Monobank\\": "src/"
        },
        "files": [
            "src/constants.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Monobank\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
